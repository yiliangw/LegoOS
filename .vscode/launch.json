{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "LegoOS debug",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            // "preLaunchTask": "start Qemu",
            "program": "${workspaceFolder}/vmImage",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "miDebuggerServerAddress": "127.0.0.1:1234",
        }
    ]
}
